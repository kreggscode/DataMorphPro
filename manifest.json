{
  "manifest_version": 3,
  "name": "DataMorphPro",
  "version": "1.0",
  "description": "Transform data formats with stunning visual effects. Convert CSV ↔ JSON ↔ SQL instantly with dark mode, drag & drop, and more!",
  "permissions": [
    "storage",
    "downloads"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "DataMorphPro - Transform Your Data",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
